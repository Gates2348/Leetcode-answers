class Solution {
    public int[] twoSum(int[] nums, int target) {

        for(int a = 0;a < nums.length - 1;a++){
            for(int j = a + 1;j < nums.length;j++){
                if(nums[a] + nums[j] == target){
                    return new int[] {a, j};
                }
            }
        }
        return null;
    }
}
